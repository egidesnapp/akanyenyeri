# Prevent execution of PHP files in uploads
<Files *.php>
    deny from all
</Files>
<Files *.phtml>
    deny from all
</Files>
<Files *.php3>
    deny from all
</Files>
<Files *.php4>
    deny from all
</Files>
<Files *.php5>
    deny from all
</Files>
<Files *.pl>
    deny from all
</Files>
<Files *.py>
    deny from all
</Files>
<Files *.jsp>
    deny from all
</Files>
<Files *.asp>
    deny from all
</Files>
<Files *.sh>
    deny from all
</Files>
